<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<QuickLabPIVng-AppContext>
    <project>
        <title>Exemple dense Liu-Shen combined with L-K</title>
        <date>2023-07-13</date>
        <description>- Simple test with single image pair of a Rankine vortex
- Computed with dense Liu-Shen combined with Lucas-Kanade</description>
        <projectType>Local</projectType>
        <remoteServer></remoteServer>
        <piv-configuration>
            <imageWidth>512</imageWidth>
            <imageHeight>512</imageHeight>
            <maskEnabled>false</maskEnabled>
            <maskOnlyAtExport>false</maskOnlyAtExport>
            <maskFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nil="true"/>
            <sourceImageFolder>/home/lpnm/Documents/HPC/QuickLabValidation/QuickLabPIV-ng/examples</sourceImageFolder>
            <sourceImageFile>/home/lpnm/Documents/HPC/QuickLabValidation/QuickLabPIV-ng/examples/rankine_vortex01_0.tif</sourceImageFile>
            <imageType>PIVImagePair</imageType>
            <imagePatternA>rankine_vortex01_0.tif</imagePatternA>
            <imagePatternB>rankine_vortex01_1.tif</imagePatternB>
            <numberOfImages>1</numberOfImages>
            <imageFilteringMode>DoNotApplyImageFiltering</imageFilteringMode>
            <imageFilterOptions/>
            <imageFilteringAfterWarpingOnly>false</imageFilteringAfterWarpingOnly>
            <initialResolution>IA128</initialResolution>
            <endResolution>IA16</endResolution>
            <clippingMode>AllowedOutOfBoundClipping</clippingMode>
            <warpingMode>BiLinearMicroWarping2ndImageWithBiCubicSplineInterpolation</warpingMode>
            <topMargin>0</topMargin>
            <bottomMargin>0</bottomMargin>
            <leftMargin>0</leftMargin>
            <rightMargin>0</rightMargin>
            <inheritanceMode>BiCubicSpline</inheritanceMode>
            <superpositionOverlapPercentage>50.0</superpositionOverlapPercentage>
            <superpositionStartStep>IA128</superpositionStartStep>
            <interpolationMode>CombinedBaseAndFinalInterpolator</interpolationMode>
            <interpolationStartStep>IA128</interpolationStartStep>
            <interpolationOptions>
                <interpolation-CombinedBaseAndFinal>
                    <interpolationMode>CombinedBaseAndFinalInterpolator</interpolationMode>
                    <subInterpolationModes>Gaussian1DHongweiGuo</subInterpolationModes>
                    <subInterpolationModes>LiuShenWithLucasKanadeOpenCL</subInterpolationModes>
                    <alsoApplyMainInterpolationOnLastStep>false</alsoApplyMainInterpolationOnLastStep>
                    <applyFinalInterpolationAsLastPIVProcessingStep>true</applyFinalInterpolationAsLastPIVProcessingStep>
                </interpolation-CombinedBaseAndFinal>
                <interpolation-Gaussian1D-HongweiGuo>
                    <interpolationMode>Gaussian1DHongweiGuo</interpolationMode>
                    <numberOfPixels>3</numberOfPixels>
                    <numberOfIterations>20</numberOfIterations>
                </interpolation-Gaussian1D-HongweiGuo>
                <interpolation-LiuShenWithLucasKanadeOpenCL>
                    <interpolationMode>LiuShenWithLucasKanadeOpenCL</interpolationMode>
                    <ignorePIVBaseDisplacements>false</ignorePIVBaseDisplacements>
                    <ignorePIVBaseMode>IgnoreUV</ignorePIVBaseMode>
                    <filterSigmaLK>2.0</filterSigmaLK>
                    <filterWidthPxLK>3</filterWidthPxLK>
                    <numberOfIterationsLK>5</numberOfIterationsLK>
                    <windowSizeLK>27</windowSizeLK>
                    <filterSigmaLS>0.48</filterSigmaLS>
                    <filterWidthPxLS>5</filterWidthPxLS>
                    <multiplierLS>1000.0</multiplierLS>
                    <numberOfIterationsLS>60</numberOfIterationsLS>
                    <vectorsWindowSizeLS>13</vectorsWindowSizeLS>
                    <denseExport>true</denseExport>
                </interpolation-LiuShenWithLucasKanadeOpenCL>
            </interpolationOptions>
            <stabilizationMode>Disabled</stabilizationMode>
            <stabilizationOptions/>
            <validationReplaceInvalidByNaNs>false</validationReplaceInvalidByNaNs>
            <validationNumberOfValidatorIterations>0</validationNumberOfValidatorIterations>
            <validationIterateUntilNoMoreReplaced>false</validationIterateUntilNoMoreReplaced>
            <validationMode>Disabled</validationMode>
            <validationOptions/>
            <validationReplacementMode>Invalid</validationReplacementMode>
        </piv-configuration>
        <export-configuration>
            <swapUVOrder>false</swapUVOrder>
            <splitExports>false</splitExports>
            <numberOfPIVMapsPerExportedFile>0</numberOfPIVMapsPerExportedFile>
        </export-configuration>
    </project>
    <executionEnv>
        <cpuThreads>4</cpuThreads>
        <enableOpenCL>true</enableOpenCL>
        <OpenCL-Devices>
            <OpenCL-Device>
                <deviceType>GPU</deviceType>
                <name>NVIDIA&lt;GPU&gt; - NVIDIA GeForce RTX 3050 Ti Laptop GPU</name>
                <id>140455900949920</id>
                <perfScore>1.0</perfScore>
                <defaultConfig>false</defaultConfig>
                <numberOfComputeUnits>20</numberOfComputeUnits>
                <threadsPerComputeUnit>128</threadsPerComputeUnit>
                <minThreadMultiple>32</minThreadMultiple>
                <greatestThreadCommonDivisor>512</greatestThreadCommonDivisor>
                <selected>true</selected>
            </OpenCL-Device>
        </OpenCL-Devices>
        <openClAssignments>140455900949920</openClAssignments>
        <openClAssignments>140455900949920</openClAssignments>
        <openClAssignments>140455900949920</openClAssignments>
        <openClAssignments>140455900949920</openClAssignments>
        <validAssignments>true</validAssignments>
        <validAssignments>true</validAssignments>
        <validAssignments>true</validAssignments>
        <validAssignments>true</validAssignments>
    </executionEnv>
</QuickLabPIVng-AppContext>
